---
import { Image } from "astro:assets";
import Layout from "../../layouts/Layout.astro";
/* Components */
import Testimonial from "../../components/Testimonial.astro";
import PartnersCard from "../../components/PartnersCard.astro";
import LogoItems from "../../components/LogoItems.astro";
import Button from "../../components/Button.astro";
import Carousel from "../../components/Carousel.astro";
/* Images */
import mockup from "../../images/mockup.png";
import Deliverect from "../../images/companies logo/Deliverect_Logo.png";
import Foodics from "../../images/companies logo/Foodics.png";
import Loginext from "../../images/companies logo/LogiNext_Logo.png";
import Makane from "../../images/companies logo/makane.png";
import Makeorder from "../../images/companies logo/Makeorder.png";
import OTO from "../../images/companies logo/oto.png";
import Salla from "../../images/companies logo/Salla.png";
import Solo from "../../images/companies logo/Solo.png";
import Taker from "../../images/companies logo/TAKER-GO-logo.png";
import Order from "../../images/companies logo/TRY ORDER.jpg";
import Slide from "../../images/companies logo/Try Slide.png";
import Zid from "../../images/companies logo/Zid.png";
import Kama from "../../images/companies logo/kama.png";
import fleet from "../../images/bg/fleeet.png";
import Mcdonald from "../../images/companies logo/clients/McDonald's.png";
import Restaurant from "../../images/bg/restaurent.png";
import app from "../../images/Group 14.png";
import order_status from "../../images/icons/benefits/integration/order status.png";
import api from "../../images/icons/benefits/integration/api.png";
import LiveLocation from "../../images/icons/benefits/integration/live location.png";
import Payment_methods from "../../images/icons/benefits/integration/payment method.png";
import chat from "../../images/icons/benefits/integration/chat.png";
import Payment from "../../images/icons/benefits/integration/order-return.png";
import truck from "../../images/icons/path-truck 1.png";
import google from "../../images/icons/google play.png";
import apple from "../../images/icons/apple store.png";
import CarouselThreeD from "../../components/CarouselThreeD.astro";

import testimonials from "../../data/testimonial.json";
import clients from "../../data/clients.json";

---

<Layout title="Home" lang="en">
  <Carousel />
  <section class="relative h-screen">
    <video
      autoplay
      muted
      loop
      class="absolute right-0 bottom-0 h-full w-full object-cover">
      <source src="../../../delivery.mp4" type="video/mp4" />
    </video>
    <div
      class="w-full h-full absolute inset-0 bg-gradient-to-t md:bg-gradient-to-br bg-black/80">
      <div
        class="container section-container flex flex-col justify-center text-center gap-5 h-full">
        <div
          class="notshow flex flex-col lg:items-center text-white"
          data-animation="animate-fade animate-duration-[1500ms]">
          <div class="space-y-1 mb-5">
            <div class="text-3xl lg:text-5xl">Streamline Your Deliveries,</div>
            <h1 class="text-primaryOrange text-5xl lg:text-7xl">
              Elevate Your Business.
            </h1>
          </div>
          <div class="leading-7 mb-5 text-sm lg:text-base lg:w-[80%]">
            We are a leading technology partner dedicated to enhancing the last
            mile delivery experience for companies focused on customer
            satisfaction. Our mission is to ensure that every delivery reaches
            its destination reliably, on-time, with precision, efficiency, and
            in the most sustainable manner possible.
          </div>
        </div>
      </div>
    </div>
  </section>
  <div class="space-y-16 w-full overflow-x-hidden py-10 bg-white">
    <h4 class="text-3xl font-medium text-center">
      <span class="text-primaryOrange font-semibold">Leajlak's</span> empowered brands
    </h4>
    <div class="logo-track">
      {clients.map((client) => <LogoItems imgSrc={client.image} alt="image" /> )}
    </div>
  </div>
  <section id="Headline1" class="overflow-x-hidden flex">
    <div
      class="notshow container section-container relative flex flex-col justify-center gap-10 lg:flex-row lg:items-center lg:justify-start"
      data-animation="animate-fade animate-duration-[1500ms]">
      <div class="flex flex-col justify-center space-y-8 lg:w-[50%] text-base">
        <div class="space-y-3">
          <div>
            <h3 class="tracking-widest">Who we are?</h3>
            <h2>A delivery management system, for all your needs.</h2>
          </div>
          <p>
            Leajlak's innovative Order Management System is the answer to
            connecting merchants and thirdparty logistics companies for seamless
            ondemand express & scheduled delivery services.
          </p>
          <p>
            Our solution bridges the gap between merchants and logistic
            companies and can significantly enhance efficiency, reduce costs,
            and improve customer satisfaction.
          </p>
        </div>
        <div>
          <Button link="/en/register" text="Register now" />
        </div>
      </div>
      <div
        class="lg:absolute lg:right-0 hidden sm:flex sm:justify-center lg:justify-start lg:w-[50%]">
        <Image
          src={mockup}
          class="w-[770px] lg:w-[870px]"
          alt="mockup"
          loading="eager"
        />
      </div>
    </div>
  </section>
  <section id="WhoWeServe" class="bg-white">
    <div
      class="container section-container flex flex-col gap-20 overflow-hidden">
      <h3
        class="notshow text-left md:text-center"
        data-animation="animate-fade animate-duration-[1500ms]">
        Who we serve?
      </h3>
      <div class="flex flex-col items-center gap-y-32">
        <div
          class="notshow flex flex-col items-center gap-10 md:flex-row w-full"
          data-animation="animate-fade animate-duration-[1500ms]">
          <div class="md:w-[30%] space-y-2 justify-self-start">
            <h2>Target Sectors</h2>
            <p>
              Our services target multiple sectors, from restaurants and cafes
              to e-groceries and much more.
            </p>
          </div>
          <div class="relative w-full md:w-[70%]">
            <CarouselThreeD />
          </div>
        </div>
        <div
          class="notshow flex flex-col items-center justify-center gap-10 md:flex-row"
          data-animation="animate-fade animate-duration-[1500ms]">
          <div class="md:w-[30%] space-y-2 md:order-2">
            <div>
              <h2>Our Service Ecosystem</h2>
            </div>
            <p>
              Explore how our platform connects these key players, enhancing
              every step of the delivery journey and driving success across the
              supply chain.
            </p>
          </div>
          <div class="md:w-[70%] relative">
            <video
              autoplay
              muted
              loop
              src="/leajlak workflow.mp4"
              class="relative w-full h-full">
            </video>
          </div>
        </div>
      </div>
    </div>
  </section>
  <section id="Benfits">
    <div
      class="container section-container flex flex-col justify-center items-center gap-20">
      <div
        class="notshow flex flex-col text-left md:text-center space-y-2 md:w-[60%]"
        data-animation="animate-fade animate-duration-[1500ms]">
        <h3 class="tracking-xl">Benfits</h3>
        <h2 class="text-lg md:text-2xl">
          Bridging Merchants and Logistics Companies for a Seamless Delivery
          Ecosystem
        </h2>
      </div>
      <div class="space-y-32">
        <!-- Merchants -->
        <div
          class="notshow relative flex flex-col lg:flex-row justify-center items-center duration-200 lg:space-x-10"
          data-animation="animate-fade animate-duration-[1500ms]">
          <div
            class="bg-[url('/src/images/bg/restaurant-mobile.png')] bg-no-repeat bg-cover p-5 space-y-5 h-full text-white lg:text-black lg:bg-none lg:w-[50%] rounded-xl">
            <div>
              <div
                class="text-primaryOrange text-lg font-medium tracking-xl uppercase">
                Leajlak for
              </div>
              <h2 class="text-white lg:text-black">Merchants</h2>
            </div>
            <p class="text-white lg:text-black">
              Leajlak's solution revolutionizes order management, addressing the
              critical challenges faced by merchants and unlocking the potential
              for a seamless, efficient delivery ecosystem.
